<% if current_user %>
	<div class="text-center">
		<h1>Welcome, <%= current_user.name %></h1>
		<h4>Logged In with <%= @auth['provider'].capitalize %></h4>
		<%= link_to "/profile", class:"btn btn-success" do %>
			WORDCLOUD
		<% end %>
	</div>
<% else %>
	<div class="text-center">
		<h1>Welcome, to WordCloud!!</h1>
		<p>Login with your account to generate your Word Cloud!!!</p>
	</div>
<% end %>